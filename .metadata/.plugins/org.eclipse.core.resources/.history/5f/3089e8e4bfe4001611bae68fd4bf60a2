package movies;

import java.util.ArrayList;

public class Movie {
	private String name;
	private ArrayList actors;
	private int year;

	public Movie(String name, int year) {
		this.name = name;
		this.year = year;
		this.actors = new ArrayList();
	}
	/**
	add Actor to the list of Actors
	@param: Actor
	 * @return 
	*/
	public void addActor(String Actor){
	this.actors.add(Actor)
	}

	/**
	Compare if object is has the same year and name
	*/
	public boolean equals(Object other){
	//if other is a movie, compare the year and name, other wiser 	//return false

	if (other instance Movie);
		return (this.name == object.name) and (this.year == object.year);
	}
	else{
	return false;
	}
	
	}
	public String getActors(){
	return this.actors
	}
	public String getName(){
	return this.name
	}
	public int getYear(){
	return this.year	
	}
//DO THISS
//Make sure that line separators are right!!
	public toString(){
	
	}

}
